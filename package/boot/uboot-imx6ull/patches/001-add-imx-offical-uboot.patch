+				usdhc2_pads, ARRAY_SIZE(usdhc2_pads));
+			usdhc_cfg[1].sdhc_clk = mxc_get_clock(MXC_ESDHC2_CLK);
+			gpio_direction_output(USDHC2_PWR_GPIO, 1);
+			break;
+		case 2:
